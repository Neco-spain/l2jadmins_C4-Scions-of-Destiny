	<skill id="7029" levels="4" name="Super Haste">
	  <!-- Done by dc - Completed by fissban-->
	  <table name="#Tab-runSpd"> 1.5 2 4 6 </table>
	  <table name="#Tab-reuse"> 1.00 1.50 1.75 1.92 </table>
	  <table name="#Tab-mAtkSpd"> 1.5 1.8 3 4 </table>
	  <table name="#Tab-pAtkSpd"> 1.2 1.5 2 3 </table>
	  <table name="#Tab-pAtk"> 1.2 2 4 6 </table>
	  <table name="#Tab-mAtk"> 1.2 2 4 6 </table>
	  <set name="target" val="TARGET_SELF"/>
	  <set name="skillType" val="BUFF"/>
	  <set name="operateType" val="ACTIVE"/>
	  <for>
	    <effect name="Buff" time="1200" count="1" val="0">
	      <mul order="0x30" stat="RUN_SPEED" val="#Tab-runSpd"/>
	      <div order="0x30" stat="PHYSICAL_SKILL_REUSE" val="#Tab-reuse"/>
	      <div order="0x30" stat="MAGICAL_SKILL_REUSE" val="#Tab-reuse"/>	      <mul order="0x30" stat="MAGICAL_ATTACK_SPEED" val="#Tab-mAtkSpd"/>
	      <mul order="0x30" stat="PHYSICAL_ATTACK_SPEED" val="#Tab-pAtkSpd"/>
	      <mul order="0x30" stat="PHYSICAL_ATTACK" val="#Tab-pAtk"/>
	      <mul order="0x30" stat="MAGICAL_ATTACK" val="#Tab-mAtk"/>
	    </effect>
	  </for>
	</skill>
